{
    "manifest_version": 2,
    "name": "URL Decoder",
    "description": "This extension decodes the URL of the current page.",
    "version": "1.0",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "permissions": ["activeTab"]
  }
  